	<queryString>
		<![CDATA[select T.TRANSACTION_ID, T.TRANSACTION_DATE, T.TOTAL_TRANSACTION_AMOUNT, TP.NAME, TP.FEE, TD.AMOUNT, TD.SENDER_ACCOUNT, A.ACCOUNT_ID, TD.RECEIVER_ACCOUNT, C. ADDRESS, TP.NAME, A.CURRENT_BALANCE, TD.CONTENT , C.FIRST_NAME || ' ' || C.LAST_NAME FULL_NAME
from (((TRANSACTION T JOIN TRANSACTION_TYPE TP USING(TRANSACTION_TYPE_ID))
        JOIN TRANSFER_DETAIL TD ON TD.TRANSACTION_ID= T.TRANSACTION_ID)
            JOIN ACCOUNT A ON A.ACCOUNT_ID = TD.RECEIVER_ACCOUNT)
                JOIN CUSTOMER C ON C.CUSTOMER_ID = A.CUSTOMER_ID]]>
